| inputlookup day03.csv
| rex field=input max_match=0 "(?<data>(do\(\)|don't\(\)|mul\(\d+,\d+\)))"
| fields data
| mvexpand data
| rex field=data "^(mul\((?<x>\d+),(?<y>\d+)\)|(?<condition>do(n't)?\(\)))"
| streamstats last(condition)
| eval result = if('last(condition)' == "don't()", null(), x * y)
| stats sum(result)
